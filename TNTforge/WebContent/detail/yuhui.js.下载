$(function(){String.prototype.tmp=function(a){return this.replace(/\$\w+\$/g,function(c){var b=a[c.replace(/\$/g,"")];return(b+"")=="undefined"?"":b})};$(".shopping-you-like .float-clearfix li>a").die("tap").live("tap",function(){var a=$(this).attr("clickurl");var b=$(this).attr("itemCode");if(a){_gsq.push(["T","GWD-002994","trackEvent","Bridgewell Recommendation","Cart",b]);window.location.href=a}});$(".shoppingcart-content .product-list-main .product-img").die("tap").live("tap",function(){var a=$(this).attr("clickurl");var b=$(this).attr("itemCode");if(a){_gsq.push(["T","GWD-002994","trackEvent","Bridgewell Recommendation","Cart",b]);window.location.href=a}})});function pdp_yuhui(){var a="";if(undefined!=$("#categoryId").val()){a=$("#categoryId").val()}(window._bwtm=window._bwtm||[]).push({mid:402,pid:"item",iid:$("#itemCode").val(),ch:a,rcat:"",uid:$("#userIdd").val(),layout:"jsarray",count:4,success:function(f){if(f===undefined||f.length===0){return}var c=f[0].length-1;var d=c;var e="",b=$("#products script[data-id='list_tpl']").html();function g(h){e+=b.tmp(h)}while(c>=0){g(f[0][d-c]);c-=1}$("#products .product-list-main").html(e);dataYuhui=$("#products").html();$("#pdp-container .article.pdp-yuhui .guess-you-like").html(dataYuhui);c=f[0].length-1;while(c>=0){$("#pdp-container .article.pdp-yuhui .guess-you-like .product-price").find("del").eq(d-c).text(accounting.formatMoney($("#pdp-container .article.pdp-yuhui .guess-you-like .product-price").find("del").eq(d-c).text(),"￥",0));$("#pdp-container .article.pdp-yuhui .guess-you-like .product-price").find("p").eq(d-c).text(accounting.formatMoney($("#pdp-container .article.pdp-yuhui .guess-you-like .product-price").find("p").eq(d-c).text(),"￥",0));if($("#pdp-container .article.pdp-yuhui .guess-you-like .hover-img").eq(d-c).attr("src")==""){$("#pdp-container .article.pdp-yuhui .guess-you-like .hover-img").eq(d-c).attr("src",$("#pdp-container .article.pdp-yuhui .guess-you-like #img-out").eq(d-c).attr("src"))}c-=1}}})}